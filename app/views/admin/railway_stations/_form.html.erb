<%= form_for [:admin, station], html: { style: "display: #{ hidden ? 'none' : 'block'}"}  do |form| %>
  <%= render 'common/errors', resourse: station %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
<p></p>
  <div class="actions">
     <%= form.submit %> 
  </div>
   
<% end %>
